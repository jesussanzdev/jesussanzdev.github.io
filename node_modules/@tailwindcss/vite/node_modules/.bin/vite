#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Usuario/Documents/Projects/retroweb/node_modules/.pnpm/vite@6.2.5_@types+node@22.1_84e7ab8c55ce42e1fe1272df7372c58b/node_modules/vite/bin/node_modules:/mnt/c/Users/Usuario/Documents/Projects/retroweb/node_modules/.pnpm/vite@6.2.5_@types+node@22.1_84e7ab8c55ce42e1fe1272df7372c58b/node_modules/vite/node_modules:/mnt/c/Users/Usuario/Documents/Projects/retroweb/node_modules/.pnpm/vite@6.2.5_@types+node@22.1_84e7ab8c55ce42e1fe1272df7372c58b/node_modules:/mnt/c/Users/Usuario/Documents/Projects/retroweb/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Usuario/Documents/Projects/retroweb/node_modules/.pnpm/vite@6.2.5_@types+node@22.1_84e7ab8c55ce42e1fe1272df7372c58b/node_modules/vite/bin/node_modules:/mnt/c/Users/Usuario/Documents/Projects/retroweb/node_modules/.pnpm/vite@6.2.5_@types+node@22.1_84e7ab8c55ce42e1fe1272df7372c58b/node_modules/vite/node_modules:/mnt/c/Users/Usuario/Documents/Projects/retroweb/node_modules/.pnpm/vite@6.2.5_@types+node@22.1_84e7ab8c55ce42e1fe1272df7372c58b/node_modules:/mnt/c/Users/Usuario/Documents/Projects/retroweb/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@6.2.5_@types+node@22.1_84e7ab8c55ce42e1fe1272df7372c58b/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@6.2.5_@types+node@22.1_84e7ab8c55ce42e1fe1272df7372c58b/node_modules/vite/bin/vite.js" "$@"
fi
